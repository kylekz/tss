{
  "name": "cosmo-web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "pnpm clean && dotenv -e .env.local vinxi dev",
    "build": "pnpm clean && dotenv -e .env.production.local vinxi build",
    "start": "dotenv -e .env.production.local && node ./.output/server/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config . --fix",
    "db:generate": "drizzle-kit generate",
    "db:upgrade": "drizzle-kit up",
    "db:migrate": "dotenv -e .env.local && tsx ./migrate.mts",
    "clean": "rimraf --glob app.config.timestamp_*"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.1.0",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@tanstack/react-router": "^1.91.3",
    "@tanstack/react-router-devtools": "npm:@tanstack/router-devtools@^1.91.3",
    "@tanstack/react-router-with-query": "^1.91.3",
    "@tanstack/start": "^1.91.3",
    "dotenv-cli": "^7.4.4",
    "next-themes": "^0.4.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^4.1.2",
    "vinxi": "^0.5.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@tailwindcss/vite": "4.0.0-beta.8",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "20.17.0",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-react-compiler": "19.0.0-beta-201e55d-20241215",
    "dotenv": "^16.4.7",
    "eslint": "9.17.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-compiler": "19.0.0-beta-201e55d-20241215",
    "eslint-plugin-react-google-translate": "^0.0.114",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-tanstack-query": "npm:@tanstack/eslint-plugin-query@^5.62.1",
    "eslint-plugin-tanstack-router": "npm:@tanstack/eslint-plugin-router@^1.85.3",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.13.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "4.0.0-beta.8",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite-plugin-tsconfig-paths": "npm:vite-tsconfig-paths@^5.1.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.1",
      "@types/react-dom": "19.0.2",
      "esbuild": "0.24.0"
    }
  }
}
